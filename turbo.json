{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint", "^build"]
    },
    "type-check": {
      "dependsOn": ["^type-check", "^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:apply": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false,
      "persistent": true
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "admin#build": {
      "env": ["NEXT_PUBLIC_API_URL", "NEXT_PUBLIC_WS_URL"]
    }
  }
}
